{
  "name": "checkmate-virtue-qa",
  "version": "1.0.0",
  "description": "Browser testing suite for CheckMate Virtue",
  "scripts": {
    "test": "python qa/run_browser_tests.py",
    "test:headed": "HEADLESS=false python qa/run_browser_tests.py",
    "test:debug": "HEADLESS=false TIMEOUT=60000 python qa/run_browser_tests.py",
    "install-playwright": "playwright install",
    "install-deps": "pip install playwright && playwright install"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0"
  },
  "keywords": [
    "testing",
    "playwright",
    "browser-automation",
    "qa"
  ],
  "author": "CheckMate Virtue Team",
  "license": "MIT"
}
